import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner x=new Scanner(System.in);
    int x1=x.nextInt();
    int y1=x.nextInt();
    int x2=x.nextInt();
    int y2=x.nextInt();
    float a=(float)(x1+x2);
    float b=(float)(y1+y2);
    System.out.printf("Binoy's house is located at (" +a/2+","+b/2+")");
       
    }
}
